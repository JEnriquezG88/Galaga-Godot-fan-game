config_version=5

[application]

config/name="Galaga-FanGame"
run/main_scene="res://levels/test.tscn"
config/features=PackedStringArray("4.3", "Forward Plus")
run/max_fps=60
boot_splash/image="res://globals/resources/icons/splash-screen.png"
config/icon="res://globals/resources/icons/icon_monochrome_dark.png"

[autoload]

States="*res://globals/scripts/states.gd"
TargetFactory="*res://globals/scripts/target_generator.gd"
DistributionGenerator="*res://globals/scripts/distribution_generator.gd"
Level="*res://globals/scripts/Level.gd"
GlobalInput="*res://globals/scripts/GlobalInput.gd"
GlobalReferences="*res://globals/scripts/global_references.gd"

[display]

window/size/viewport_width=1280
window/size/viewport_height=1280
window/size/resizable=false
window/size/borderless=true

[file_customization]

folder_colors={
"res://enemies/": "red",
"res://globals/": "blue",
"res://levels/": "orange",
"res://player/": "green"
}

